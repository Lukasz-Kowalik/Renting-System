@{
    ViewData["Title"] = "Your rented items";
}
<div class="sticky-top text-center d-none">
    <h2 class=" m-0 p-0 text-danger bg-secondary ">
        You have to return item
    </h2>
</div>
<div class="vh-100 container-fluid">
    <div class="row">
        @Html.Partial("_Menu")

        <div class="col-lg-10 p-5 col-md-8 col-sm-12 p-0  d-flex  flex-column">
            <div>
                <h1>Your rented items:</h1>
            </div>
            <div class="bg-secondary p-2">
                <table id="Rented-table" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                        <tr>
                            <th>RentId</th>
                            <th>ItemId</th>
                            <th>Name</th>
                            <th>Quantity</th>
                            <th>Category</th>
                            <th>Rent date</th>
                            <th>When should be returned</th>
                            <th>Return date</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script defer src="~/js/Rented.js"></script>
@{
    ViewData["Title"] = "Admin panel";

}
<div class="vh-100 container-fluid">
    <div class="row">
        @Html.Partial("_Menu")
        <div class="col-lg-10 p-5 col-md-8 col-sm-12 p-0  d-flex  flex-column">
            <div>
                <h1>Users:</h1>
            </div>
            <div>
                <table id="User-table" class="table table-striped table-bordered" style="width:100%">
                    <thead>
                        <tr>
                            <th>UserId</th>
                            <th>FirstName</th>
                            <th>LastName</th>
                            <th>Email</th>
                            <th>Role</th>
                        </tr>
                    </thead>
                    <tbody></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script defer src="~/js/AdminPanel.js"></script>
@{
    ViewData["Title"] = "Cart";
}
<div class="vh-100 container-fluid">
    <div class="row">
        @Html.Partial("_Menu")
        <div class="col-lg-10 pt-5 col-md-8 col-sm-12 p-0 vh-100 d-flex justify-content-center ">
            <table id="Cart-table" class="table table-striped table-bordered" style="width:100%">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Name</th>
                        <th>Quantity</th>
                        @*<th>Remove</th>*@
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>